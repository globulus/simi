def load(case):
    file = File("/Users/gordanglavas/Downloads/game_inputs/game_input" + case)
    reader = ReadStream(file)
    reader.readLine()
    input = reader.readLine()
    array = input.split(' ').map(def s: s.toNumber())
    rescue ex:
        if ex:
            print ex.message
            return nil
        end
    end
    reader.close()
    return array
end

def numFactors($n):
    $count = 0
    while $n % 2 == 0:
        $n /= 2
        $count += 1
    end
    for i in Range(3, Math.sqrt($n) + 1, 2):
        while $n % i == 0:
            $n /= i
            $count += 1
        end
    end
    if $n > 2: $count += 1
    return $count
end

